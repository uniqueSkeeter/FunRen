		}
		if (this.getClass() != obj.getClass()) {
			return false;
		}
		final StationEntity other = (StationEntity) obj;
		if (this.address == null) {
			if (other.address != null) {
				return false;
			}
		} else if (!this.address.equals(other.address)) {
