	/**
	 * 日结号
	 */
	@Column(name = "`RJID`")
	private Integer rjid;

	/**
	 * 备用
	 */
	@Column(name = "`BZ`")
	private String bz;

	/**
	 * 网点号
	 */
	@Column(name = "`STATIONNO`")
