	public void setBz(String newBz) {
		bz = newBz;
	}

	/**
	 * 网点号的设定
	 * 
	 * @param newString 网点号
	 */
	public void setStationno(String newStationno) {
		stationno = newStationno;
	}

	/**
	 * 创建时间 （日期）的设定
	 * 
	 * @param newTimestamp 创建时间 （日期）
	 */
	public void setCreateDate(Timestamp newCreateDate) {
