     * @return Integer 班结号
     */
    public Integer getBjid() {
        return bjid;
    }


    /**
     * 日结号的取得
     *
     * @return Integer 日结号
     */
    public Integer getRjid() {
        return rjid;
    }


    /**
     * 支付类型的取得
     *
     * @return String 支付类型
     */
    public String getRectype() {
        return rectype;
    }


    /**
     * 备用的取得
     *
     * @return String 备用
     */
    public String getBz() {
        return bz;
    }


    /**
     * 网点的取得
     *
     * @return String 网点
     */
    public String getStationno() {
        return stationno;
    }


    /**
     * 创建时间
（日期）的取得
     *
     * @return Timestamp 创建时间
（日期）
     */
    public Timestamp getCreateDate() {
        return createDate;
    }


    /**
     * 操作员编号的取得
     *
     * @return Integer 操作员编号
     */
    public Integer getUserid() {
        return userid;
    }


