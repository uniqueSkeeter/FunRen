package com.fr.station.common.bean;

public class BlackCardBean implements StandardBean {

	private static final long serialVersionUID = 1L;

	/**
	 * cardID 主键 blackcard(黑卡)
	 */
	private Integer cardId;

	/**
	 * create_date(创建日期) blackcard(黑卡)
	 */
	private String createDate;

	/**
	 * stationNo(网点号) blackcard(黑卡)
	 */
	private String stationNo;

	/**
	 * rjid(日结号) blackcard(黑卡)
	 */
	private String dayNumber;

	/**
	 * rjrq(日结时间) blackcard(黑卡)
	 */
	private String dayTime;

	public Integer getCardId() {
		return this.cardId;
	}

	public void setCardId(Integer cardId) {
		this.cardId = cardId;
	}

	public String getCreateDate() {
		return this.createDate;
	}

	public void setCreateDate(String createDate) {
		this.createDate = createDate;
	}

	public String getStationNo() {
		return this.stationNo;
	}

	public void setStationNo(String stationNo) {
		this.stationNo = stationNo;
	}

	public String getDayNumber() {
		return this.dayNumber;
	}

	public void setDayNumber(String dayNumber) {
		this.dayNumber = dayNumber;
	}

	public String getDayTime() {
		return this.dayTime;
	}

	public void setDayTime(String dayTime) {
		this.dayTime = dayTime;
	}

	@Override
	public String toString() {
		return "BlackCardBean [cardId=" + this.cardId + ", createDate=" + this.createDate + ", stationNo=" + this.stationNo
				+ ", dayNumber=" + this.dayNumber + ", dayTime=" + this.dayTime + "]";
	}

}
